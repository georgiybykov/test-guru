<tbody>
  <tr>
    <td class="font-weight-bold"><%= test.title %></td>
    <td><%= test_level(test) %></td>
    <td><%= test.questions.count %></td>

    <td><%= link_to t('.show_test_button'), admin_test_path(test), class: 'btn btn-primary' %></td>
    <td><%= link_to t('.edit_test_button'), edit_admin_test_path(test), class: 'btn btn-primary' %></td>

    <td>
      <%= link_to t('.delete_test_button'),
                  admin_test_path(test),
                  method: :delete,
                  data: { confirm: t('.are_you_sure') },
                  class: 'btn btn-danger' %>
    </td>
  </tr>
</tbody>
